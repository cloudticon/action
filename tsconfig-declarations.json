{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "cloudticon": ["src/index.ts"]
    },
    "target": "es5",
    "declaration": true,
    "emitDeclarationOnly": true,
    "experimentalDecorators": true,
    "outDir": "lib",
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
}